{
    "network": "xception_224",
    "method_name": "vallina",
    "nb_classes": 2,
    "dropout": 0.0,
    "img_size": 224,
    "normalize": {
        "mean": [0.5, 0.5, 0.5],
        "std": [0.5, 0.5, 0.5]
    },
    "optimizer": {
        "batch_size": 256,
        "type": "Adam",
        "momentum": 0.9,
        "weight_decay": 1e-4,
        "blr": 2e-4,
        "min_lr": 0,
        "warmup_epochs": 0,
        "epochs": 13,
        "criterion": "CrossEntropyLoss",
        "scheduler": "StepLR"
    }
}